{
  "name": "csharp-class-view",
  "displayName": "C# Class View",
  "description": "C# Class View displays solution projects, namespaces, classes, interfaces and enumerations in a tree view.",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manojbaishya/vscode-csharp-class-view"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manojbaishya/vscode-csharp-class-view/issues"
  },
  "homepage": "https://github.com/manojbaishya/vscode-csharp-class-view",
  "categories": [
    "Programming Languages",
    "Visualization",
    "Education",
    "Other"
  ],
  "keywords": [
    "dotnet",
    ".net",
    "c#",
    "namespace",
    "hierarchy"
  ],
  "activationEvents": [
    "workspaceContains:**/.sln"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "csharpClassView",
          "title": "C# Class Explorer",
          "icon": "resources/dark/csharp-class-view.png"
        }
      ]
    },
    "views": {
      "csharpClassView": [
        {
          "id": "csharpClassView",
          "name": "C# Class View",
          "type": "tree",
          "icon": "resources/dark/csharp-class-view.png",
          "contextualTitle": "Namespace Explorer"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "csharpClassView",
        "contents": "Compiling solution. Please wait..."
      }
    ],
    "commands": [
      {
        "command": "csharpClassView.collapseCsharpClassExplorer",
        "title": "Collapse C# Class View",
        "icon": "$(collapse-all)"
      },
      {
        "command": "csharpClassView.refreshCsharpClassExplorer",
        "title": "Refresh C# Class View",
        "icon": {
          "light": "resources/light/refresh.png",
          "dark": "resources/dark/refresh.png"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "csharpClassView.collapseCsharpClassExplorer",
          "when": "view == csharpClassView",
          "group": "navigation"
        },
        {
          "command": "csharpClassView.refreshCsharpClassExplorer",
          "when": "view == csharpClassView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "build": "node esbuild.js --production",
    "compile": "pnpm run check-types && pnpm run lint && pnpm run build",
    "package": "pnpm run compile && pnpm vsce package --no-dependencies",
    "vscode:prepublish": "pnpm run package",
    "publish": "pnpm vsce publish --no-dependencies",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "compile-tests": "tsc -p . --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "watch-tests": "tsc -p . -w --outDir out",
    "test": "vscode-test",
    "clean": "rimraf ./dist ./out"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.50.0",
    "@bufbuild/protobuf": "^2.2.3",
    "@bufbuild/protoc-gen-es": "^2.2.3",
    "@connectrpc/connect": "^2.0.1",
    "@connectrpc/connect-node": "^2.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "~20.17.16",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@9.15.4+sha256.9bee59c7313a216722c079c1e22160dea7f88df4e0c3450b1d7b01b882336c6a"
}