{
	"compilerOptions": {
		"strict": true,
		"module": "CommonJS",
		"target": "ES2020",
		"lib": [
			"ES2020"
		],
		"sourceMap": true,
		"rootDir": "src",
		"sourceRoot": "src",
		"declaration": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true,
		"noImplicitThis": true,
		"esModuleInterop": true
	},
	"exclude": [
		"node_modules"
	]
}